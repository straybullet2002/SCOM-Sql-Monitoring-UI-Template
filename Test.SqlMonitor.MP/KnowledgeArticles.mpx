              <maml:list>
                <maml:listItem>
                  <maml:para>Целочисленный: bigint, int, smallint, tinyint</maml:para>
                </maml:listItem>
                <maml:listItem>
                  <maml:para>Логический: bit</maml:para>
                </maml:listItem>
                <maml:listItem>
                  <maml:para>С плавающей точкой: float, real</maml:para>
                </maml:listItem>
                <maml:listItem>
